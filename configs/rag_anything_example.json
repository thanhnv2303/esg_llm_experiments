{
  "rag_anything": {
    "working_dir": "artifacts/rag_storage",
    "parser": "mineru",
    "parse_method": "auto",
    "enable_image_processing": true,
    "enable_table_processing": true,
    "enable_equation_processing": false
  },
  "llm": {
    "provider": "openai",
    "model": "gpt-4o-mini",
    "api_base": "https://api.openai.com",
    "temperature": 0.0,
    "top_p": 1.0,
    "api_key": "REPLACE_WITH_OPENAI_KEY"
  },
  "embedding": {
    "model": "text-embedding-3-large",
    "embedding_dim": 3072,
    "max_token_size": 8192,
    "api_key": "REPLACE_WITH_OPENAI_KEY",
    "input_type": "document",
    "extra_params": {
      "input_type": "passage",
      "extra_body": {
        "truncate": "NONE"
      }
    }
  },
  "vision_model": {
    "provider": "openai",
    "model": "gpt-4o",
    "api_key": "REPLACE_WITH_OPENAI_KEY"
  },
  "lightrag": {
    "vector_storage": "NanoVectorDBStorage",
    "kv_storage": "JsonKVStorage",
    "graph_storage": "NetworkXStorage",
    "doc_status_storage": "JsonDocStatusStorage",
    "chunk_token_size": 400,
    "chunk_overlap_token_size": 40,
    "input_type": "passage"
  },
  "document_processing": {
    "parse_method": "auto",
    "display_stats": false,
    "device": "cpu",
    "device_enforce": false,
    "device_virtual_memory_gb": null,
    "parser_kwargs": {}
  },
  "query": {
    "mode": "hybrid",
    "use_multimodal": true,
    "params": {
      "temperature": 0.0
    }
  }
}
